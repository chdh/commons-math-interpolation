{
   "compilerOptions": {
      "allowUnreachableCode": false,
      "baseUrl": "src",
      "lib": ["ES6", "DOM"],
      "module": "ES6",
      "moduleResolution": "node",
      "noFallthroughCasesInSwitch": true,
      "noImplicitReturns": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "outDir": "tempBuild",
      "paths": {
         "commons-math-interpolation/*": ["../../dist/*"]
      },
      "removeComments": true,
      "strict": true,
      "target": "ES2017"
   },
   "include": [
      "src/**/*"
   ]

}
